<h1> Make a new note here </h1>

<%= simple_form_for @notes do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.association :category %>
  <%= f.input :public, :as => :select, :collection => [['No',false],['Yes',true]], :include_blank => false, :label => "Public"%>
  <%= f.button :submit %>
<% end %>